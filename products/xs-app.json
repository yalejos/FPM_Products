{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "sap/opu/odata/sap/SEPMRA_SO_MAN2/",
      "target": "sap/opu/odata/sap/SEPMRA_SO_MAN2/",
      "destination": "3034_FPM",
      "authenticationType": "none",

<mvc:View 
controllerName="com.ya.products.ext.view.Review"
xmlns="sap.m"
xmlns:core="sap.ui.core" 
xmlns:mvc="sap.ui.core.mvc"  
xmlns:table="sap.ui.table"
xmlns:macros="sap.fe.macros">
    <Page id="Review" title="{i18n>ReviewTitle}">
        <content>
           <table:Table rows="{sales>/results}">
                <table:columns>
                    <table:Column>
                        <Label text="ID" />
                        <table:template><Text text="{sales>ID}" /></table:template>
                    </table:Column>
                    <table:Column>
                        <Label text="Year" />
                        <table:template><Text text="{sales>year}" /></table:template>
                    </table:Column>
                    <table:Column>
                        <Label text="Month" />
                        <table:template><Text text="{sales>month}" /></table:template>
                    </table:Column>
                    <table:Column>
                        <Label text="Quantity" />
                        <table:template><Text text="{sales>quantitySales}" /></table:template>
                    </table:Column>
                </table:columns>
            </table:Table>
        </content>
    </Page>
</mvc:View>
<core:FragmentDefinition
                      xmlns="sap.m" 
					  xmlns:core="sap.ui.core"  
					  xmlns:macros="sap.fe.macros"
					  >
	<VBox id="chartContainerVBox">
        <Text id="chartHeaderText" text="{i18n>salesAnalysis}" />
        <macros:Chart 
            id="ProductSalesAnalyticalChart"
			contextPath="toSales" 
			metaPath="@com.sap.vocabularies.UI.v1.Chart"
            selectionChange=".extension.com.ya.products.ext.controller.ProductObjectPage.onSelectionChange"
        />
    </VBox>
</core:FragmentDefinition>